  <main id="main" class="main">
        <div class="container-fluid">
            <h1 class="mb-4">Add City Information</h1>
            <form action="#" method="POST">
                <div class="row g-3">
                    <div class="col-md-6">
                        <label for="cityId" class="form-label">City ID</label>
                        <input type="text" class="form-control" id="cityId" name="cityId" required>
                    </div>
                    <div class="col-md-6">
                        <label for="stateId" class="form-label">State ID</label>
                        <input type="text" class="form-control" id="stateId" name="stateId" required>
                    </div>
                    <div class="col-md-6">
                        <label for="countryId" class="form-label">Country ID</label>
                        <input type="text" class="form-control" id="countryId" name="countryId" required>
                    </div>
                    <div class="col-md-6">
                        <label for="cityName" class="form-label">City Name</label>
                        <input type="text" class="form-control" id="cityName" name="cityName" required>
                    </div>
                    <div class="col-md-6">
                        <label for="stdCode" class="form-label">STD Code</label>
                        <input type="text" class="form-control" id="stdCode" name="stdCode" required>
                    </div>
                    <div class="col-md-6">
                        <label for="pinCode" class="form-label">Pin Code</label>
                        <input type="text" class="form-control" id="pinCode" name="pinCode" required>
                    </div>
                    <div class="col-md-6">
                        <label for="creationDate" class="form-label">Creation Date</label>
                        <input type="date" class="form-control" id="creationDate" name="creationDate" required>
                    </div>
                    <div class="col-md-6">
                        <label for="userId" class="form-label">User ID</label>
                        <input type="text" class="form-control" id="userId" name="userId" required>
                    </div>
                    <div class="col-12">
                        <button type="submit" class="btn btn-primary">Submit</button>
                    </div>
                </div>
            </form>
        </div>
        
    </main>
